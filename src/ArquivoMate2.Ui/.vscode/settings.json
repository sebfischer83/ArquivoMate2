{
    "angular-schematics.schematicsDefaultOptions": {
        "angular-*": {
            "style": "scss",
            "externalTemplate": true
        },
        "angular-component": {
            "skipDisplayBlock": true,
            "ngOnInit": false
        },
        "angular-page": {
            "skipNgOnInit": false
        }
    },
    "i18n-ally.localesPaths": [
        "src/assets/i18n"
    ],
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.keystyle": "flat"
}